{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "ecu",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"EcuAppOData": {
				"uri": "/ecu-web/ODataService.svc",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "ecu.view.NewApp",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ecu.i18n.i18n"
				}
			},
			"user": {
				"dataSource": "EcuAppOData",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ecu.view",
				"controlId": "newapp",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "newApp",
				"target": "newapp"
			}, {
				"pattern": "corpo",
				"name": "Corpo",
				"target": "corpo"
			}, {
				"pattern": "floor1",
				"name": "Floor1",
				"target": "floor1"
			},
			{
				"pattern": "floor2",
				"name": "Floor2",
				"target": "floor2"
			},
			{
				"pattern": "floor3",
				"name": "Floor3",
				"target": "floor3"
			},
			{
				"pattern": "roomDetails",
				"name": "RoomDetails",
				"target": "roomDetails"
			},
			{
				"pattern":"manager",
				"name":"managerLogin",
				"target":"managerLogin"
			},
			{
				"pattern":"dashboard",
				"name":"dashboard",
				"target":"dashboard"
			},
						{
				"pattern":"dashboardSettings",
				"name":"dashboardSettings",
				"target":"dashboardSettings"
			},
			{
				"pattern":"dashboard/Tiles_1/:arg1:",
				"name":"Tiles_1",
				"target":"Tiles_1"
			},
						{
				"pattern":"dashboard/Tiles_2/:arg1:/:arg2:/",
				"name":"Tiles_2",
				"target":"Tiles_2"
			},
			{
				"pattern":"manager/managerpanel",
				"name":"managerPanel",
				"target":"managerPanel"
			},
			{
				"pattern":"manager/managerpanel/salesplane/:groupby:",
				"name":"salesPlane",
				"target":"salesPlane"
			},
			{
				"pattern":"manager/managerpanel/totalsales/:groupby:/:chartType:",
				"name":"totalSales",
				"target":"totalSales"
			},
			{
				"pattern":"manager/managerpanel/map/:bu:",
				"name":"map",
				"target":"map"
			},
			{
				"pattern":"manager/managerpanel/map2/:bu:",
				"name":"map2",
				"target":"map2"
			}
			],
			"targets": {
				"newapp": {
					"viewName": "NewApp",
					"viewLevel" : 1,
					"transition": "slide"
				},
				"corpo": {
					"viewName": "Corpo",
					"transition": "slide"
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				},
				"floor1": {
					"viewName": "Floor1",
					"transition": "slide"
				},
				"floor2": {
					"viewName": "Floor2",
					"transition": "slide"
				},
				"floor3": {
					"viewName": "Floor3",
					"transition": "slide"
				},
				"roomDetails": {
					"viewName": "RoomDetails",
					"transition": "slide"
				},
				"managerLogin":{
					"viewName":"manager/ManagerLogin",
					"transition": "slide"
				},
				"dashboard":{
					"viewName":"manager/Dashboard",
					"transition":"slide"
				},
				"dashboardSettings":{
					"viewName":"manager/DashboardSettings",
					"transition":"slide"
				},
				"Tiles_1":{
					"viewName":"manager/reports/Tiles_1",
					"transition": "slide"
				},
				"Tiles_2":{
					"viewName":"manager/reports/Tiles_2",
					"transition": "slide"
				},
				"map":{
					"viewName":"manager/reports/Map",
					"transition": "slide"
				},
				"map2":{
					"viewName":"manager/reports/Map2",
					"transition": "slide"
				},
				"managerPanel":{
					"viewName":"manager/ManagerPanel",
					"transition": "slide"
				},
				"salesPlane":{
					"viewName":"manager/reports/SalesPlan",
					"transition": "slide"
				},
				"totalSales":{
					"viewName":"manager/reports/TotalSales",
					"transition": "slide"
				}
			}
		}
	}
}
