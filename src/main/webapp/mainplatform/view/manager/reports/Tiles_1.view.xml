							width="160px" height="30px" />
						<m:ToolbarSpacer />
					</tnt:ToolHeader>

					<m:TileContainer id="tileContener"></m:TileContainer>

				</m:content>

			</m:Page>

		</m:pages>
	</m:App>
</mvc:View>