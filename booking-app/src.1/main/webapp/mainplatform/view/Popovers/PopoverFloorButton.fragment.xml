<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core">
    <Popover showHeader="false" contentWidth="240px" contentHeight="256px" placement="Top">
        <NavContainer id="navFloorButton">
            <Page id="master" showHeader="false">
                <List id="list">
                    <StandardListItem title="Reservations" description="" type="Active" icon="sap-icon://message-information" iconDensityAware="false" iconInset="false" press="onShowStatus"/>
					<StandardListItem title="{i18n>Floor.popover.floorUp}" description="" type="Active" icon="sap-icon://arrow-top" iconDensityAware="false" iconInset="false" press="onFloorUp"/>
                    <StandardListItem title="{i18n>Floor.popover.floorDown}" description="" type="Active" icon="sap-icon://arrow-bottom" iconDensityAware="false" iconInset="false" press="onFloorDown"/>
                    <StandardListItem title="Resize" description="" type="Active" icon="sap-icon://resize" iconDensityAware="false" iconInset="false" press="onResizeMap" />
				</List>
            </Page>
        </NavContainer>
    </Popover>
</core:FragmentDefinition>